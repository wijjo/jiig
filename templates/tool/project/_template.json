{
  "templates": [
    {
      "path": "bin/mytool",
      "output_path": "bin/$mytool",
      "description": "$mytool main script (jiig-run version)",
      "expansion": "word",
      "executable": true
    },
    {
      "path": "bin/mytool.py",
      "output_path": "bin/$mytool.py",
      "description": "$mytool main script (direct Python version)",
      "expansion": "word",
      "executable": true
    },
    {
      "path": "__init__.py",
      "output_path": "__init__.py",
      "expansion": "word"
    },
    {
      "path": "mytool/__init__.py",
      "output_path": "$mytool/__init__.py",
      "expansion": "word"
    },
    {
      "path": "mytool/tasks/__init__.py",
      "output_path": "$mytool/tasks/__init__.py",
      "expansion": "word"
    },
    {
      "path": "mytool/tasks/root.py",
      "output_path": "$mytool/tasks/root.py",
      "expansion": "word"
    },
    {
      "path": "mytool/tasks/calc.py",
      "output_path": "$mytool/tasks/calc.py",
      "expansion": "word"
    },
    {
      "path": "mytool/tasks/case.py",
      "output_path": "$mytool/tasks/case.py",
      "expansion": "word"
    },
    {
      "path": "mytool/tasks/words.py",
      "output_path": "$mytool/tasks/words.py",
      "expansion": "word"
    }
  ]
}
