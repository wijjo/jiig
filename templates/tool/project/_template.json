{
  "templates": [
    {
      "path": "template_tool_name",
      "output_path": "$template_tool_name",
      "expansion": "word",
      "executable": true
    },
    {
      "path": "lib/__init__.py",
      "output_path": "lib/__init__.py",
      "expansion": "word"
    },
    {
      "path": "lib/template_tool_name/__init__.py",
      "output_path": "lib/$template_tool_name/__init__.py",
      "expansion": "word"
    },
    {
      "path": "lib/template_tool_name/task/__init__.py",
      "output_path": "lib/$template_tool_name/task/__init__.py",
      "expansion": "word"
    },
    {
      "path": "lib/template_tool_name/task/root.py",
      "output_path": "lib/$template_tool_name/task/root.py",
      "expansion": "word"
    },
    {
      "path": "lib/template_tool_name/task/template_task_name.py",
      "output_path": "lib/$template_tool_name/task/$template_task_name.py",
      "expansion": "word"
    }
  ]
}
