{
  "templates": [
    {
      "path": "mytool",
      "output_path": "$mytool",
      "expansion": "word",
      "executable": true
    },
    {
      "path": "lib/__init__.py",
      "output_path": "lib/__init__.py",
      "expansion": "word"
    },
    {
      "path": "lib/mytool/__init__.py",
      "output_path": "lib/$mytool/__init__.py",
      "expansion": "word"
    },
    {
      "path": "lib/mytool/task/__init__.py",
      "output_path": "lib/$mytool/task/__init__.py",
      "expansion": "word"
    },
    {
      "path": "lib/mytool/task/root.py",
      "output_path": "lib/$mytool/task/root.py",
      "expansion": "word"
    },
    {
      "path": "lib/mytool/task/mytask.py",
      "output_path": "lib/$mytool/task/$mytask.py",
      "expansion": "word"
    }
  ]
}
