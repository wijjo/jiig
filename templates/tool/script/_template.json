{
  "templates": [
    {
      "path": "mytool",
      "output_path": "$mytool",
      "description": "$mytool stand-alone script (jiig-run version)",
      "expansion": "word",
      "executable": true
    },
    {
      "path": "mytool.py",
      "output_path": "$mytool.py",
      "description": "$mytool stand-alone script (direct Python version)",
      "expansion": "word",
      "executable": true
    }
  ]
}
